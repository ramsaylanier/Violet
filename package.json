{
  "name": "violet",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "concurrently \"npm run lint\" \"npm run format\" \"npm run typecheck\"",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:api": "tsx watch src/api/server.ts",
    "dev:client": "vite dev --port 3000",
    "build": "vite build",
    "start": "npm run start:api & npm run start:client",
    "start:api": "tsx src/api/server.ts",
    "start:client": "vite preview --port 3000",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt",
    "format:check": "oxfmt -c",
    "emulators": "firebase emulators:start --import .firebase/emulator-export",
    "emulators:ui": "firebase emulators:start --only ui",
    "emulators:export": "firebase emulators:export .firebase/emulator-export",
    "seed": "tsx scripts/seed.ts",
    "seed:emulator": "USE_EMULATORS=true tsx scripts/seed.ts --emulators"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@base-ui/react": "^1.0.0",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@octokit/rest": "^22.0.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-router": "^1.56.0",
    "@tanstack/react-router-devtools": "^1.147.1",
    "@tanstack/router-vite-plugin": "^1.56.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "dotenv-flow": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.21.2",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.0.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "react-resizable-panels": "^4.3.3",
    "recharts": "^2.15.4",
    "shadcn": "^3.6.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zustand": "^5.0.1",
    "tmp": "^0.2.3",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
