    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "firebase-tools": "^13.0.0",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^6.0.4"
  }
}
